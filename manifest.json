{
	"name": "Autosave Online",
	"manifest_version": 2,
	"version": "0.0.1",
	"content_scripts": [{
			"matches": ["<all_urls>"],
			"js": ["saveFormsWithListeners.js"],
			"all_frames": true,
			"run_at": "document_idle"
		}],
	"permissions": ["storage"],
	"browser_action": {
			"browser_style": true,
			"default_icon": "verified-text-paper.svg",
			"default_popup": "accessSavedData.html"
		}
}